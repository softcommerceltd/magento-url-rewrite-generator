<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <!-- CLI -->
    <type name="Magento\Framework\Console\CommandListInterface">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="softcommerce_url_generate_category" xsi:type="object">SoftCommerce\UrlRewriteGenerator\Console\Command\Category</item>
                <item name="softcommerce_url_generate_product" xsi:type="object">SoftCommerce\UrlRewriteGenerator\Console\Command\Product</item>
            </argument>
        </arguments>
    </type>
    <type name="SoftCommerce\UrlRewriteGenerator\Console\Command\Category">
        <arguments>
            <argument name="urlRewrite" xsi:type="object">SoftCommerce\UrlRewriteGenerator\Model\CategoryUrlRewrite</argument>
        </arguments>
    </type>
    <type name="SoftCommerce\UrlRewriteGenerator\Console\Command\Product">
        <arguments>
            <argument name="urlRewrite" xsi:type="object">SoftCommerce\UrlRewriteGenerator\Model\ProductUrlRewrite</argument>
        </arguments>
    </type>
</config>
